{
  "name": "@a1/hfhs-integration-tests",
  "description": "HFHS Integration Tests",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clean": "node clean.js",
    "cucumber:reporter": "node cucumber-reporter.js",
    "test:open": "./node_modules/.bin/cypress open",
    "test:run": "./node_modules/.bin/cypress run",
    "lint": "eslint --ext .js --fix ./cypress",
    "build": "npm run clean && npm run test:run & npm run cucumber:reporter",
    "flow": "flow check"
  },
  "devDependencies": {
    "cucumber-html-reporter": "^5.0.2",
    "cypress": "^3.6.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "^2.18.2",
    "flow-bin": "^0.102.0"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-flow": "^7.0.0",
    "@cypress/browserify-preprocessor": "^2.1.1",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "cypress-cucumber-preprocessor": "^1.16.2",
    "cypress-xpath": "^1.4.0",
    "moment": "^2.24.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "fs-extra": "latest"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/support/step_definitions/",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
